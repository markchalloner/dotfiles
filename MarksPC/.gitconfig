[user]
	name = Mark Challoner
	email = mark.a.r.challoner@gmail.com
	#signingkey = 0x58EABDB1293D248B!
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[core]
	excludesfile = ~/.gitexcludes
[push]
	default = nothing
[alias]
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[commit]
	gpgsign = true
[merge]
	tool = vimdiff
	conflictstyle = diff3
[mergetool]
	prompt = false
	keepBackup = false
[gpg]
	program = /cygdrive/c/Program Files (x86)/GNU/GnuPG/gpg2.exe
