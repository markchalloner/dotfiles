# Navigation
alias pushdev="func_pushd ~/Development"
alias pushwa="func_pushd ${WA_ROOT}"
alias pushmu="func_pushd ~/Vagrant/marks-ubuntu-trusty-vm"
alias cdwa="builtin cd ${WA_ROOT}"
alias cdapi="builtin cd ${WA_ROOT}/wa-api"
alias cdarch="builtin cd ${WA_ROOT}/wa-arch"
alias cddoc="builtin cd ${WA_ROOT}/wa-api-documentation"
alias cdbackend="builtin cd ${WA_ROOT}/wa-backend"
alias cdhermes="builtin cd ${WA_ROOT}/wa-hermes"
alias cdinfra="builtin cd ${WA_ROOT}/wa-infrastructure"
alias cdwatmp="builtin cd ${WA_ROOT}/tmp"
alias cdzeus="builtin cd ${WA_ROOT}/wa-zeus"
# Vagrant
alias vsshwa="func_vagrant_ssh "${HOME}/Vagrant/wa""
alias vsshmu="func_vagrant_ssh "${HOME}/Vagrant/marks-ubuntu-trusty-vm""
# Git
alias gitbwa="(cdwa && gitb)"
alias gitpwa="(cdwa && gitp)"
# SSH
alias sshlocal="vsshwa"
alias sshdev="ssh -i ${HOME}/.ssh/wa-dev.pem ubuntu@dev.api.workivate.com"
alias sshqa="ssh -i ${HOME}/.ssh/wa-qa.pem ubuntu@qa.api.workivate.com"
